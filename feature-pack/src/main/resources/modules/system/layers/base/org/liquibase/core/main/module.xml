<?xml version="1.0" ?>
<module xmlns="urn:jboss:module:1.9" name="org.liquibase.core">
    <exports>
        <include path="META-INF/services/**"/>
        <include path="META-INF/**"/>
    </exports>
    <resources>
        <artifact name="${org.liquibase:liquibase-core}"/>
    </resources>
    <dependencies>
        <module name="java.base"/>
        <module name="java.logging"/>
        <module name="java.sql"/>
        <module name="java.naming"/>
        <module name="java.desktop"/>
        <module name="jakarta.servlet.api"/>
        <module name="javax.sql.api"/>
        <module name="jakarta.xml.bind.api"/>
        <module name="org.apache.commons.io"/>
        <module name="org.apache.commons.lang3" export="true"/>
        <module name="org.apache.commons.collections4"/>
        <module name="org.slf4j" services="import"/>
        <module name="org.slf4j.impl" services="import"/>
        <module name="org.jboss.logging"/>
        <module name="org.yaml.snakeyaml"/>
        <module name="com.opencsv"/>
    </dependencies>
</module>
